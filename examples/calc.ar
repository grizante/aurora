func calc(op, a, b) 
desc "It's a sample of calculator" {
  if op equal "+" {
    return a + b
  }

  if op equal "-" {
    return a - b
  }

  if op equal "*" {
    return a * b
  }

  if op equal "/" {
    return a / b
  }
}

var result = calc("+", 4, 4)
print(result)

var result = calc("-", 4, 4)
print(result)

var result = calc("/", 4, 4)
print(result)

var result = calc("*", 4, 4)
print(result)
